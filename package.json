{
  "name": "starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint:all": "nx run-many --target=lint --project=all",
    "test:all": "nx run-many --target=test --project=all",
    "e2e:all": "nx run-many --target=e2e --projects=e2e --configuration=production",
    "build:all": "nx run-many --target=build --projects=ui,api --configuration=production",
    "prod": "npm run lint:all && npm run build:all && npm run test:all && npm run e2e:all",
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "docker compose -f tools/scripts/docker-compose.yml rm dev-db -sfv",
    "db:dev:up": "docker compose -f tools/scripts/docker-compose.yml up dev-db -d",
    "db:dev": "npm run db:dev:rm && npm run db:dev:up && sleep 1 && npx prisma generate && npm run prisma:dev:deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.1.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "~14.1.0",
    "@angular/compiler": "~14.1.0",
    "@angular/core": "~14.1.0",
    "@angular/forms": "~14.1.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "~14.1.0",
    "@angular/platform-browser-dynamic": "~14.1.0",
    "@angular/router": "~14.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.0.5",
    "@ngrx/store": "^14.3.0",
    "@nrwl/angular": "14.5.2",
    "argon2": "^0.28.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.1.0",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "~14.1.0",
    "@angular/language-service": "~14.1.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "14.5.2",
    "@nrwl/cypress": "14.5.2",
    "@nrwl/eslint-plugin-nx": "14.5.2",
    "@nrwl/jest": "14.5.2",
    "@nrwl/linter": "14.5.2",
    "@nrwl/nest": "14.5.2",
    "@nrwl/node": "14.5.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "14.5.2",
    "@prisma/client": "^4.1.1",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cypress": "^10.2.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "nx": "14.5.2",
    "prettier": "^2.6.2",
    "prisma": "^4.1.1",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  },
  "prisma": {
    "schema": "apps/api/prisma/schema.prisma"
  }
}
